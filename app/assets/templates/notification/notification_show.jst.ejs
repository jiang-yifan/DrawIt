<div class="notification-wrapper group
  <%= notification.escape('status') %>">
  <div class='notification-avatar'>
    <img src="<% notification.escape('avatar_url') %>"/>
  </div>

  <div class='notification-content'>
    <span><%= notification.escape("content") %></span>
  </div>
</div>
<div class='notification-modal-container'></div>
